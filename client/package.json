{
  "name": "iap-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch:scss": "node-sass src/app/azure-pages/styles/main.scss src/app/azure-pages/styles.css -w",
    "watch:azure": "live-server src/app/azure-pages",
    "generate:doc": "compodoc -p src/tsconfig.app.json --disableRoutesGraph --theme readthedocs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.3.1",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/pwa": "^0.12.3",
    "@angular/router": "^7.2.2",
    "@angular/service-worker": "^7.2.2",
    "@azure/msal-angular": "0.1.2",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/entity": "^7.2.0",
    "@ngrx/schematics": "^7.1.0",
    "@ngrx/store": "^7.1.0",
    "@ngrx/store-devtools": "^7.1.0",
    "@schuchard/prettier": "^2.0.2",
    "core-js": "^2.5.4",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat-css": "^3.1.0",
    "gulp-copy": "^4.0.1",
    "gulp-image": "^5.1.0",
    "gulp-minify": "^3.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-typescript": "^5.0.1",
    "hammerjs": "^2.0.8",
    "merge-stream": "^2.0.0",
    "ngx-gravatar": "^7.2.2",
    "ngx-logger": "^3.3.12",
    "path": "^0.12.7",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "seneca-angular-error-handler": "file:dist/seneca-angular-error-handler",
    "seneca-angular-server-error-interceptor": "file:dist/seneca-angular-server-error-interceptor",
    "systemjs-builder": "^0.16.15",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.2",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "fs-extra": "^7.0.1",
    "husky": "^2.4.0",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^8.1.7",
    "ng-bullet": "^1.0.3",
    "ngrx-store-freeze": "^0.2.4",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.1",
    "typescript": "~3.2.2",
    "webpack-dev-server": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "ng build --prod"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
